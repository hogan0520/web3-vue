    "@web3-vue-org/eip1193": "0.0.11-alpha.0",
    "@web3-vue-org/empty": "0.0.11-alpha.0",
    "@web3-vue-org/metamask": "0.0.11-alpha.0",
    "@web3-vue-org/network": "0.0.11-alpha.0",
    "@web3-vue-org/types": "0.0.11-alpha.0",
    "@web3-vue-org/url": "0.0.11-alpha.0",
    "@web3-vue-org/walletconnect": "0.0.11-alpha.0",
